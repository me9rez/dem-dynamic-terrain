{
  "name": "@me9rez/dem-dynamic-terrain",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.6.1",
  "description": "使用 GDAL 制作地形瓦片，支持 mapbox 和 terrarium 两种编码输出格式",
  "author": "me9rez",
  "license": "MIT",
  "keywords": [
    "dem",
    "terrain",
    "terrain-generator",
    "gdal",
    "gdal-async",
    "mapbox-gl",
    "maplibre-gl"
