  "devDependencies": {
    "@antfu/eslint-config": "^4.8.1",
    "@microsoft/api-extractor": "^7.51.1",
    "@rslib/core": "^0.5.3",
    "@types/node": "^22.13.9",
    "eslint": "9.22.0",
    "tsx": "^4.19.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "gdal-async"
